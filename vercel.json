{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/manifest.json", "dest": "/public/manifest.json" },
    { "src": "/sw.js", "dest": "/public/sw.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
